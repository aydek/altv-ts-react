{
    "name": "altv-ts-reac",
    "version": "5.0.1",
    "description": "This is gamemode",
    "scripts": {
        "update": "altv-pkg d rc",
        "build:server": "node --es-module-specifier-resolution=node build/server",
        "build:client": "node --es-module-specifier-resolution=node build/client",
        "dev:server": "node --es-module-specifier-resolution=node build/server -dev",
        "dev:client": "node --es-module-specifier-resolution=node build/client -dev",
        "after-any": "node --es-module-specifier-resolution=node build/copy",
        "dev": " npm-run-all -p dev:* tsc && npm run after-any",
        "build": "npm-run-all -p build:* && npm run after-any",
        "upd-altv-types": "npm install @altv/types-shared@latest @altv/types-client@latest @altv/types-server@latest @altv/types-natives@latest",
        "tsc:server": "cd src/server && npx tsc -w",
        "tsc:client": "cd src/client && npx tsc -w",
        "tsc": "npm-run-all -p tsc:*",
        "webviews": "npx vite ./src-webviews --clearScreen=false --host=localhost --port=3000",
        "build:webviews": "npx vite build ./src-webviews"
    },
    "author": "aydek",
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^2.5.3",
        "@altv/types-natives": "^1.5.3",
        "@altv/types-server": "^2.6.6",
        "@altv/types-shared": "^1.4.2",
        "@altv/types-webview": "^1.0.5",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "altv-pkg": "^2.0.11",
        "autoprefixer": "^10.4.14",
        "fs-extra": "^11.1.1",
        "glob": "^10.3.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.27",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^1.14.0",
        "tailwindcss": "^3.3.3",
        "typescript": "latest"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "printWidth": 120,
        "arrowParens": "always",
        "singleQuote": true
    },
    "type": "module",
    "dependencies": {
        "@dnd-kit/core": "^6.0.8",
        "altv-esbuild": "^0.4.12",
        "concurrently": "^8.2.0",
        "mongoose": "^7.4.1",
        "vite": "^4.3.1"
    },
    "engines": {
        "node": ">=18"
    }
}
